{
	"welcomeFile": "index.html",
	"authenticationMethod": "route",
	"logout": {
		"logoutEndpoint": "/do/logout"
	},
	"routes": [{
		"source": "^/r5web/(.*)$",
		"target": "$1",
		"localDir": "resources"
	}, {
		"source": "^/node/(.*)$",
		"destination": "r5-backend",
		"csrfProtection": true,
		"authenticationType": "xsuaa"
	}, {
		"source": "/(.*)",
		"authenticationType": "xsuaa",
		"localDir": "resources"
	}]
}